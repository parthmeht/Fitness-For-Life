        <% include ./partials/header.ejs %>
        <% include ./partials/user-navigation.ejs %>
        <div class="row row-override">
            <% include ./partials/site-navigation.ejs %>
            <main class="col-sm-9 text-center">
                <form class="form-signin" method="POST" action="/login">
                    <h4 class="mb-3 font-weight-normal">Welcome to Fitness for Life</h4>
                    <h6 class="mb-3 font-weight-normal">Please enter your username and password to sign in.</h6>
                    <label for="inputEmail" class="sr-only">Email address</label>
                    <input type="email" id="inputEmail" name="inputEmail" class="form-control" placeholder="Email address" required autofocus>
                    <label for="inputPassword" class="sr-only">Password</label>
                    <input type="password" id="inputPassword" name="inputPassword" class="form-control" placeholder="Password" required>
                    <% if(data.errorMessage!=undefined) { %>
                    <h6 class="mb-3 font-weight-normal alert alert-danger" role="alert"><%=data.errorMessage%></h6>
                    <% } %>
                    <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
                </form>
            </main>
        </div>
        <% include ./partials/footer.ejs %>
    </div>
</body>
</html>